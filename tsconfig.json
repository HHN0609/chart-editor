{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "outDir": "./dist",
        "allowJs": true,
        "sourceMap": true,
        "target": "ES6",
        "module": "ESNext",
        "jsx": "preserve",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "typeRoots": [
            "./globalDeclare"
        ]
    }
}